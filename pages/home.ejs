<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Home Online</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <!-- Favicon -->
    <link type="image/png" href="/favicon.ico" rel="icon">


    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

</head>

<body class=" bg-dark text-white">
  <nav class="navbar sticky-top navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="#"><%= user  %></a>

    </button>
  
  
      <ul class="navbar-nav mr-auto mt-2 mt-lg-0"></ul>
      <form class="form-inline" action="logout" method="POST">
        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Logout</button>
      </form>
  
  </nav>

              <div class=" bg-dark text-white">
                <div class="card-body p-5 text-center">
                  <% if (error) { %>
                    <div class="alert alert-danger" role="alert">
                      <p>error getting configs, please try again</p>
                    </div>
                  <%} else { %>
                    <% config.forEach(button=> { %>
                      <div class="form-outline form-white mb-4">
                      <button class="btn btn-outline-light btn-lg px-5" onclick="sendaction('<%= token %>', '<%= button.id %>')" id="<%= button.id %>" type="button" ><%= button.text %></button>
                      </div>
                    <% }) %>
                  <% } %>
                </div>
              </div>

    <!-- Template Javascript -->
